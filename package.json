    "eslint": "^5.8.0",
    "everything.js": "^1.0.3",
    "expect.js": "0.3.1",
    "mocha": "5.2.0",
    "shift-parser-expectations": "2018.0.2",
    "shift-parser": "7.0.0"
  },
  "keywords": [
    "Shift",
    "AST",
    "node",
    "codegen",
    "code",
