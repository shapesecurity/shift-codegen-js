{
  "name": "shift-codegen",
  "version": "7.0.3",
  "description": "code generator for Shift format ASTs",
  "author": "Shape Security",
  "homepage": "https://github.com/shapesecurity/shift-codegen-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/shapesecurity/shift-codegen-js.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
